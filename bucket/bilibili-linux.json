    "homepage": "https://github.com/msojocs/bilibili-linux",
    "license": {
        "identifier": "MIT"
    },
    "depends": "7zip",
    "url": "https://github.com/msojocs/bilibili-linux/releases/download/v1.17.2-1/bilibili-1.17.2-x64.exe#/dl.7z",
    "pre_install": [
        "7z x \"$dir\\`$PLUGINSDIR\\*.7z\" -o\"$dir\\`$PLUGINSDIR\\bilibili\""，
        "Remove-Item -R -Path $dir\\resources",
        "Move-Item $dir\\`$PLUGINSDIR\\bilibili\\* $dir -force"，
        "Remove-Item -R -Path $dir\\`$PLUGINSDIR"
    ],
    "bin": "bilibili.exe",
    "shortcuts": [
        [
            "bilibili.exe",
            "哔哩哔哩"
        ]
    ],
    "checkver": {
        "url": "https://github.com/msojocs/bilibili-linux/releases",
        "regex": "/releases/tag/v(?<version>.+?)\""
    }
}
